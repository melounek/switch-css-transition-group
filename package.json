  "devDependencies": {
    "@storybook/react": "^3.1",
    "@storybook/storybook-deployer": "^2.0.0",
    "babel-cli": "^6.24.1",
    "babel-plugin-transform-react-jsx": "^6.24",
    "babel-preset-env": "^1.6",
    "babel-preset-react-app": "^3.0.1",
    "storybook-router": "^0.2"
  },
  "dependencies": {
    "react": "^15.6",
    "react-dom": "^15.6",
    "react-router-dom": "^4.1"
  }
